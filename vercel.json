{
  "crons": [
    {
      "path": "/api/cron/sync/every-5-min",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/sync/every-15-min",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/sync/every-30-min",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/sync/hourly",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/sync/daily",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/sync/weekly",
      "schedule": "0 0 * * 0"
    },
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/health-check",
      "schedule": "*/10 * * * *"
    }
  ],
  "functions": {
    "app/api/cron/sync/[frequency]/route.ts": {
      "maxDuration": 300
    },
    "app/api/cron/cleanup/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/health-check/route.ts": {
      "maxDuration": 30
    }
  }
}